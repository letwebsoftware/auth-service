    "drizzle-orm": "^0.44.3",
    "fastify": "^5.4.0",
    "mysql2": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/fastify-cors": "^0.1.0",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "drizzle-kit": "^0.31.4",
